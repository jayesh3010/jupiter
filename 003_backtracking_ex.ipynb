   "source": [
    "def bitstr(n,s):\n",
    "    #print(n,s)\n",
    "    if n == 1: return s\n",
    "    else: return [digit + bits for digit in bitstr(1,s) for bits in bitstr(n-1,s)]\n",
    "\n",
    "print(bitstr(3,'abc'))"
   ]
